  "scripts": {
    "dev": "npm run start",
    "start": "npx webpack-dev-server --mode=development --config=webpack.config.js --hot --inline",
    "build": "rm -rf build && npm i && webpack --mode=production --progress ",
    "build:prod": "rm -rf build && npm i && cross-env IS_REAL_PROD=true webpack --mode=production --progress",
    "lint": "tsc; eslint --ext .js,.jsx,.ts,.tsx src",
    "dll": "rm -rf vendor/dist && npm run dll:dev && npm run dll:prod",
    "dll:prod": "webpack --mode=production --config=webpack.config.dll.js",
    "dll:dev": "webpack --mode=development --config=webpack.config.dll.js",
    "postdll": "echo dll已打包完成，请及时发布该dll包并更新index.ejs,详细说明请查看 wepack.dll.js !",
    "analyzer:dev": "webpack --mode=development --env.analyzer --progress",
    "analyzer:build": "webpack --mode=production --env.analyzer --progress",
    "dll:ship": "cship -p -b vendor/dist -prefix react-dva-ts-boilerplate vendor/dist/**/*",
    "ship": "npm run build && cship build/* && cship build/*/**",
    "ship:prod": "npm run build:prod && cship -p build/* && cship -p build/*/**",
    "prepublishOnly": "exit 1",
    "test": "cross-env NODE_ENV=test karma start",
    "test:server": "cross-env NODE_ENV=test karma start --single-run=false",
    "test:run": "cross-env NODE_ENV=test karma run"
  },
